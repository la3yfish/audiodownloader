{
  "audio": {
    "codec": "mp3",
    "quality": "320",
    "sample_rate": "48000"
  },
  "paths": {
    "links_file": "links.txt",
    "output_dir": "d:/mp3_yt-dlp",
    "log_file": "audiodownloader.log"
  },
  "behavior": {
    "skip_existing": true,
    "quiet_download": true
  },
  "logging": {
    "level": "INFO",
    "console_level": "ERROR",
    "format": "%(asctime)s - %(levelname)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S"
  }
}

